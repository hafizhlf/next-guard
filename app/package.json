{
  "name": "wireguard-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx wireguard.ts && next dev --turbo",
    "build": "next build",
    "start": "tsx wireguard.ts && next start",
    "lint": "next lint",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "db:create:migration": "sequelize-cli migration:generate --name",
    "db:create:seed": "sequelize-cli seed:generate --name"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "next": "14.2.18",
    "next-auth": "^4.24.10",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "qrcode.react": "^4.1.0",
    "react": "^18",
    "react-dom": "^18",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.18",
    "postcss": "^8",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
